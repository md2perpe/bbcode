<?xml version="1.0"?>
<RDF:RDF xmlns:RDF="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:chrome="http://www.mozilla.org/rdf/chrome#">

  <!-- list all the locales being supplied -->
  <RDF:Seq about="urn:mozilla:locale:root">
    <RDF:li resource="urn:mozilla:locale:ja-JP"/>
  </RDF:Seq>

  <!-- locale information -->
  <RDF:Description about="urn:mozilla:locale:ja-JP"
                   chrome:displayName="Japanese (JP)"
                   chrome:name="ja-JP">
    <chrome:packages>
      <RDF:Seq about="urn:mozilla:locale:ja-JP:packages">
        <RDF:li resource="urn:mozilla:locale:ja-JP:bbcode"/>
      </RDF:Seq>
    </chrome:packages>
  </RDF:Description>

  <!-- additional information for ja-JPM locale -->
  <RDF:Description about="urn:mozilla:locale:ja-JP" chrome:name="ja-JPM">
    <chrome:packages>
      <RDF:Seq about="urn:mozilla:locale:ja-JPM:packages">
        <RDF:li resource="urn:mozilla:locale:ja-JPM:bbcode"/>
      </RDF:Seq>
    </chrome:packages>
  </RDF:Description> 
</RDF:RDF>
