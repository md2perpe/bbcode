<?xml version="1.0"?>
<RDF:RDF xmlns:RDF="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:chrome="http://www.mozilla.org/rdf/chrome#">

  <!-- list all the locales being supplied -->
  <RDF:Seq about="urn:mozilla:locale:root">
    <RDF:li resource="urn:mozilla:locale:sv-SE"/>
  </RDF:Seq>

  <!-- locale information -->
  <RDF:Description about="urn:mozilla:locale:sv-SE"
                   chrome:displayName="Swedish"
                   chrome:name="sv-SE">
    <chrome:packages>
      <RDF:Seq about="urn:mozilla:locale:sv-SE:packages">
        <RDF:li resource="urn:mozilla:locale:sv-SE:bbcode"/>
      </RDF:Seq>
    </chrome:packages>
  </RDF:Description>

</RDF:RDF>
